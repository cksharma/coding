#include <iostream>
#include <string>
#include <vector>
using namespace std;


bool isPallin(string s, int index) {
	for (int i = 0; i < (int)s.length() / 2; i++) {
		if (i == index && index < s.length() / 2) i++;
		else if (i == index && index > s.length() / 2) i--;
		else if (s[i] != s[s.length() - 1 - i]) 
			return false;
	}
	return true;
}

vector<int> getIndex(string s) {
	int i = 0;
	int j = s.size() - 1;
	cout << " i = " << i << " j = " << j << endl;
	vector <int> v;
	cout << " s = " << s[2] << endl;
	while (i < j) {
		cout << " i = " << s[i] << "  j = " << s[j] << endl;
		if (s[i] != s[j]) break;
		//cout << " i = " << i << "  j = " << j << endl;
		i++;
		j--;
	}
	v.push_back(i);
	v.push_back(j);
	return v;
}

int main() {
	int T; cin >> T;
	while (T-- > 0) {
		string s; cin >> s;
		vector<int> v = getIndex(s);
		/*if (isPallin(s, -1)) {
			cout << "YES" << endl;
		} else {
			bool ok = isPallin(s, v[0]);
			ok = ok || isPallin(s, v[1]);
			if (ok) cout << "YES" << endl;
			else cout << "NO" << endl;
		}*/
	}
}
